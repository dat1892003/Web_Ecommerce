<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>21T1020123 - @ViewBag.Title</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="Free HTML Templates" name="keywords">
    <meta content="Free HTML Templates" name="description">

    <link href="~/themes/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" rel="stylesheet">

    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
@*     <link href="~/themes/lib/animate/animate.min.css" rel="stylesheet"> *@
    <link href="~/themes/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/themes/css/daterangepicker.css"/>
    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/themes/css/style.css" rel="stylesheet">

    <!-- Date Range Picker CSS -->
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />
    <style>
        input [type="number"]::-webkit-outer-spin-button,
        input[type="number"]::-webkit-inner-spin-button {
            -webkit-appearance: none;
            margin: 0;
        }
    </style>
</head>
<body>
    <!-- Topbar Start-->
    @{
        await Html.RenderPartialAsync("_Topbar");
    }
    <!-- Topbar End-->
    <!-- Navbar Start -->
    @{
        await Html.RenderPartialAsync("_Navbar");
    }
    <!-- Navbar End -->
    <!-- Carousel Start -->
    <!-- Carousel End -->
    <!-- Featured Start -->
    <section style="min-height:700px">
    @RenderBody()
    </section>
    <!-- Footer Start -->
    @{
        await Html.RenderPartialAsync("_Footer");
    }
    <!-- Footer End -->
    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.6.4.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.inputmask/3.3.4/jquery.inputmask.bundle.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="/themes/lib/easing/easing.min.js"></script>
    <script src="/themes/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Contact Javascript File -->
    <script src="~/themes/mail/jqBootstrapValidation.min.js"></script>
    <script src="~/themes/mail/contact.js"></script>

    <!-- Template Javascript -->
    <script src="~/themes/js/main.js"></script>
 
    <!--Date Picker Js-->
            <script  src="https://cdn.jsdelivr.net/jquery/latest/jquery.min.js"></script>
            <script  src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
            <script  src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.daterange-picker').daterangepicker({
                locale: {
                    format: 'DD/MM/YYYY'
                }
            });
        })
    </script>
    @RenderSection("scripts", false);
</body>

</html>